    agent any

    stages {
        stage('PULL') {
            steps {
                echo 'Pulling'
            }
        }
        stage('BUILD') {
            steps {
                echo 'Building application'
            }
        }
        stage('SONAR') {
            steps {
                echo 'PERFORMING SONAR ANALYSIS'
            }
        }
    }
}

